{
    "name": "t3docs/intersphinx",
    "description": "Intersphinx extension for the doctrine rst parser",
    "license": "MIT",
    "keywords": [
        "rst",
        "restructuredtext"
    ],
    "homepage": "https://github.com/TYPO3-Documentation/intersphinx",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/rst-parser": "^0.6@dev",
        "rector/rector": "^0.15.1"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.4",
        "doctrine/coding-standard": "^10.0",
        "ergebnis/composer-normalize": "2.28.*",
        "gajus/dindent": "^2.0.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^9.5",
        "symfony/css-selector": "4.4 || ^5.2 || ^6.0",
        "symfony/dom-crawler": "4.4 || ^5.2 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "T3Docs\\Intersphinx\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "T3Docs\\Tests\\Intersphinx\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
